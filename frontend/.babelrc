{
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "module-resolver",
        {
          "root": ["./src"],
          "alias": {
            "redux": "./src/redux",
            "src": "./src",
            "page": "./src/page",
            "component": "./src/component",
            "data": "./src/data",
            "assets": "./src/assets",
            "modal": "./src/modal",
            "layout": "./src/layout",
            "config": "./src/config",
            "scss": "./src/scss",
            "route": "./src/route"
          }
        }
      ]
    ]
  }
  